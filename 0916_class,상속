class Person:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def say_hello(self, to_name):
        print("안념!     "+     to_name + "나는" + self.name)

    def intorduce(self):
        print("내이름은" + self.name + "그리고 나는" + self.age + "살이야")

wonie = Person("워니", "20살")
wonie.intorduce()
wonie.say_hello("지아")
